    "jsdoc-to-markdown": "^2.0.0",
    "nyc": "^8.3.1",
    "postcss": "^5.2.4",
    "postcss-scss": "^0.3.1",
    "standard": "^8.4.0",
    "standard-changelog": "0.0.1",
    "sugarss": "^0.2.0"
  },
  "ava": {
    "verbose": "true"
  },
  "nyc": {
    "all": true,
    "include": [
      "lib"
    ],
    "extension": [
      ".js"
    ]
  },
  "keywords": [
    "postcss",
    "postcss-options"
  ],
